<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

	<servlet>
		<servlet-name>FeedProcessorServlet</servlet-name>
		<servlet-class>com.pla.textfeed2images.FeedProcessorServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ImageServlet</servlet-name>
		<servlet-class>com.pla.textfeed2images.ImageServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>FeedProcessorServlet</servlet-name>
		<url-pattern>/go</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ImageServlet</servlet-name>
		<url-pattern>/ImageServlet</url-pattern>
	</servlet-mapping>

</web-app>